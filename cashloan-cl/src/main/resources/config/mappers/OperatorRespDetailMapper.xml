<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- cl_operator_resp_detail表:运营商认证通知详情表模块 -->
<mapper namespace="com.rongdu.cashloan.cl.mapper.OperatorRespDetailMapper">
    <!--返回Map-->        
    <resultMap id="BaseResultMap" type="com.rongdu.cashloan.cl.domain.OperatorRespDetail">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="req_log_id" property="reqLogId" jdbcType="BIGINT" />
        <result column="task_id" property="taskId" jdbcType="VARCHAR" />
        <result column="operator_data" property="operatorData" jdbcType="VARCHAR" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
    </resultMap>
    
    <!--基本的sql查询字段 公共引用...-->
    <sql id="Base_Column_List">
        id,req_log_id,task_id,operator_data,create_time
    </sql>

    <!-- 基本的sql查询条件公共引用 -->
    <sql id="searchBy">
        <trim prefix="where" prefixOverrides="and|or">
            <if test=" id != null and id != '' ">
                id  = #{id,jdbcType=BIGINT}
            </if>
            <if test="reqLogId != null and reqLogId != '' ">
                and req_log_id = #{reqLogId,jdbcType=BIGINT}
            </if>
            <if test="taskId != null and taskId != '' ">
                and task_id = #{taskId,jdbcType=VARCHAR}
            </if>
            <if test="operatorData != null and operatorData != '' ">
                and operator_data = #{operatorData,jdbcType=VARCHAR}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime,jdbcType=TIMESTAMP}
            </if>
        </trim>
    </sql>
    
    
    <insert id="save" parameterType="com.rongdu.cashloan.cl.domain.OperatorRespDetail">
        insert into cl_operator_resp_detail(req_log_id,task_id,operator_data,create_time)values(#{reqLogId,jdbcType=BIGINT},#{taskId,jdbcType=VARCHAR},#{operatorData,jdbcType=VARCHAR},#{createTime,jdbcType=TIMESTAMP})
    </insert>

    
    <update id="update" parameterType="com.rongdu.cashloan.cl.domain.OperatorRespDetail">
        update cl_operator_resp_detail set 
            req_log_id = #{reqLogId,jdbcType=BIGINT},
            task_id = #{taskId,jdbcType=VARCHAR},
            operator_data = #{operatorData,jdbcType=VARCHAR},
            create_time = #{createTime,jdbcType=TIMESTAMP}
        where id = #{id ,jdbcType=INTEGER}
    </update>


    <update id="updateSelective"  parameterType="java.util.HashMap">
        update cl_operator_resp_detail
          <set>
            <if test="reqLogId != null and reqLogId != '' ">        
                req_log_id = #{reqLogId,jdbcType=BIGINT},                
            </if>
            <if test="taskId != null and taskId != '' ">
                task_id = #{taskId,jdbcType=VARCHAR},
            </if>
            <if test="operatorData != null and operatorData != '' ">
                operator_data = #{operatorData,jdbcType=VARCHAR},
            </if>
            <if test="createTime != null">
                create_time = #{createTime,jdbcType=TIMESTAMP}
            </if>
        </set>    
        where id = #{id ,jdbcType=INTEGER}
    </update>


    <select id="findByPrimary" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List" />
        from cl_operator_resp_detail
        where id = #{id,jdbcType=INTEGER}
    </select>

	<select id="findSelective" resultMap="BaseResultMap" parameterType="java.util.HashMap">
        select
        <include refid="Base_Column_List" />
        from cl_operator_resp_detail
        <include refid="searchBy"/>
    </select>
               
    <select id="listSelective" resultMap="BaseResultMap" parameterType="java.util.HashMap">
        select
        <include refid="Base_Column_List" />
        from cl_operator_resp_detail
        <include refid="searchBy"/>
    </select>


    <select id="getByTaskId" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from cl_operator_resp_detail where task_id = #{taskId,jdbcType=VARCHAR}
    </select>
</mapper>
